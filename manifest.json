{
    "manifest_version": 2,
    "name": "Documentation",
    "version": "0.1.0",
    "permissions": ["activeTab"],
    "browser_action": {   // Les paramètres
        "default_popup": "popup.html" // Le popup qui s'affichera quand vous cliquez sur l'îcone
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
